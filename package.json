{
  "name": "@rare-dev/gitc",
  "module": "gitc.ts",
  "version": "0.0.7",
  "homepage": "https://github.com/devravinder/gitc",
  "author": {
    "name": "Ravinder Reddy",
    "url": "https://github.com/devravinder"
  },
  "private": false,
  "type": "module",
  "description": "A simple CLI utility that copy git files fom git url",
  "scripts": {
    "dev": "bun gitc.ts",
    "build": "bun build ./gitc.ts --outdir dist",
    "build:standalone": "bun build ./gitc.ts --outfile dist/gitc --compile",
    "push": "bun publish"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ],
  "bin":{
    "gitc":"./dist/gitc.js"
  }
}
