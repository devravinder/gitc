{
  "name": "@rare-dev/gitc",
  "module": "gitc.ts",
  "version": "1.0.1",
  "homepage": "https://github.com/devravinder/gitc",
  "author": {
    "name": "Ravinder Reddy",
    "url": "https://github.com/devravinder"
  },
  "private": false,
  "type": "module",
  "description": "copy git files using cmd",
  "scripts": {
    "dev": "bun gitc.ts",
    "build": "bun build ./gitc.ts --outdir dist",
    "build:standalone": "bun build ./gitc.ts --outfile dist/gitc --compile",
    "push": "bun run build && bun publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "peerDependencies": {
  },
  "files": [
    "dist"
  ],
  "bin":{
    "gitc":"./dist/gitc.js"
  },
  "keywords": [
    "gitc",
    "@rare-dev",
    "@rare-dev/gitc",
    "rare-dev",
    "git copy",
    "git folder copy"
  ]
}
